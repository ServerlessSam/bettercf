{
  "HelloWorldAppLambdaRole": {
    "Type": "AWS::IAM::Role",
    "Properties": {
      "RoleName": "HelloWorldAppLambdaRole",
      "AssumeRolePolicyDocument": {
        "Statement": [
          {
            "Effect": "Allow",
            "Principal": {
              "Service": "lambda.amazonaws.com"
            },
            "Action": "sts:AssumeRole"
          }
        ]
      }
    }
  }
}